<h1>Listing email_list_items</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Email list</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @email_list_items.each do |email_list_item| %>
  <tr>
    <td><%= email_list_item.name %></td>
    <td><%= email_list_item.email %></td>
    <td><%= email_list_item.email_list_id %></td>
    <td><%= link_to 'Show', email_list_item %></td>
    <td><%= link_to 'Edit', edit_email_list_item_path(email_list_item) %></td>
    <td><%= link_to 'Destroy', email_list_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Email list item', new_email_list_item_path %>
